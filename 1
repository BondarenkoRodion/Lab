import argparse

parser = argparse.ArgumentParser()


parser.add_argument('x', type=int)
parser.add_argument('f', type=str)
parser.add_argument('y', type=int)

args = parser.parse_args()
x = args.x;
f = args.f;
y = args.y;


if args.f == '+':
    print(x + y)
elif args.f == '-':
    print(x - y)
elif args.f == '*':
    print(x * y)
elif args.f == '/':
    print(x / y)
else:
    print("Unknown operator")
