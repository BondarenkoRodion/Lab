import argparse

parser = argparse.ArgumentParser()


parser.add_argument('f')
parser.add_argument('x', type=int)
parser.add_argument('y', type=int)

args = parser.parse_args()
f = args.f;
x = args.x;
y = args.y;


if args.f == 'add':
    print(x + y)

if args.f == 'subtract':
    print(x - y)

if args.f == 'multiply':
    print(x * y)

if args.f == 'divide':
    print(x / y)
