import argparse
import math
import operator

parser = argparse.ArgumentParser()

parser.add_argument('f')
parser.add_argument('x', type=float, nargs='+')

args = parser.parse_args()
f = args.f
x = args.x

try:
    a = getattr(operator, f)
    print(a(*x))
except AttributeError:
    try:
        a = getattr(math, f)
        print(a(*x))
    except AttributeError:
        print("失敗")

