import argparse
import math
import operator

parser = argparse.ArgumentParser()

parser.add_argument('f')
parser.add_argument('x', type=float, nargs='+')

args = parser.parse_args()
f = args.f
x = args.x

try:
    if hasattr(operator, f):
        a = getattr(operator, f)
        print(a(*x))
    elif hasattr(math, f):
        a = getattr(math, f)
        print(a(*x))
except Exception:
    print("失敗")

